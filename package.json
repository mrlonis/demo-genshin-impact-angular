{
  "name": "demo-genshin-impact-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier": "prettier --config .prettierrc --write \"projects/**/*.{ts,js,css,scss,html,json}\"",
    "test": "ng test",
    "test:ci": "npm run test -- --watch=false --code-coverage --reporters progress,coverage,junit --browsers ChromeHeadless",
    "lint": "ng lint demo-genshin-impact-angular --fix",
    "format:write": "prettier projects/**/*.{ts,json,scss,html} --write",
    "format:test": "prettier projects/**/*.{ts,json,scss,html} --list-different",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/demo-genshin-impact-angular/stats.json",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run demo-genshin-impact-angular:storybook",
    "build-storybook": "ng run demo-genshin-impact-angular:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.9",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "^13.3.9",
    "@angular/compiler": "^13.3.9",
    "@angular/core": "^13.3.9",
    "@angular/forms": "^13.3.9",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "^13.3.9",
    "@angular/platform-browser-dynamic": "^13.3.9",
    "@angular/router": "^13.3.9",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.6",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/bundled-angular-compiler": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular-eslint/utils": "^13.2.1",
    "@angular/cli": "^13.3.6",
    "@angular/compiler-cli": "^13.3.9",
    "@angular/elements": "^13.3.9",
    "@babel/core": "^7.18.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/angular": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^16.11.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@typescript-eslint/types": "^5.26.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^8.0.1",
    "jasmine-core": "^4.1.1",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^12.4.2",
    "ng-lint-staged": "^12.0.3",
    "prettier": "^2.6.2",
    "puppeteer": "^14.1.1",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,scss,html,css,md,json}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "ng-lint-staged lint --fix --"
    ]
  },
  "description": "This project is an example Angular project using Genshin Impact data",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrlonis/demo-genshin-impact-angular"
  },
  "keywords": [
    "demo",
    "genshin",
    "impact",
    "angular"
  ],
  "author": "mrlonis",
  "license": "UNLICENSED"
}
